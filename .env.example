# SSELFIE Studio Environment Variables

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/sselfie

# Authentication
STACK_AUTH_PROJECT_ID=your-project-id
STACK_AUTH_PUBLISHABLE_CLIENT_KEY=your-publishable-key
STACK_AUTH_SECRET_SERVER_KEY=your-secret-key

# AI Services
REPLICATE_API_TOKEN=your-replicate-token
OPENAI_API_KEY=your-openai-key

# AWS S3 Storage
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_REGION=us-east-1
AWS_S3_BUCKET=your-bucket-name

# Payment Processing
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key
STRIPE_WEBHOOK_SECRET=your-webhook-secret

# Feature Flags
INPAINT_ENABLED=1
MAYA_CHAT_ENABLED=1
VIDEO_GENERATION_ENABLED=1

# Application
NODE_ENV=development
PORT=3000