# SSELFIE Studio - Environment Variables Example
# Copy this file to .env and set your actual values

# Core Application
NODE_ENV=development
PORT=3000

# Database
DATABASE_URL=your_database_url_here

# Stack Auth
STACK_AUTH_PROJECT_ID=your_stack_auth_project_id
STACK_AUTH_SECRET_KEY=your_stack_auth_secret_key

# Payment Processing
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Image Processing & AI
UPSCALE_PROVIDER=real_esrgan
REPLICATE_API_TOKEN=
TOPAZ_API_KEY=

# Feature Flags
VEO3_ENABLED=0
TALKING_AVATAR=0
CONTROLNET_ENABLED=0
BRAND_ASSETS_ENABLED=0

# AI Services
GOOGLE_GENAI_API_KEY=

# Voice/Avatar Services  
ELEVENLABS_API_KEY=
HEYGEN_API_KEY=

# File Storage
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret