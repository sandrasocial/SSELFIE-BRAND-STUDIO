# SSELFIE Studio Environment Variables

# Core Services
NODE_ENV=development
GOOGLE_API_KEY=your_google_api_key_here
REPLICATE_API_TOKEN=your_replicate_token_here

# Database
DATABASE_URL=your_database_url_here

# Authentication
STACK_AUTH_SECRET_SERVER_KEY=your_stack_auth_key_here

# Feature Flags
STORYBOARD_ENABLED=1  # Enable multi-scene video storyboard feature

# Video Generation
VEO_GOOGLE_MODEL=veo-2.0-generate-001
VEO_MODEL_VERSION=latest

# Storage
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
S3_BUCKET_NAME=your_s3_bucket_name