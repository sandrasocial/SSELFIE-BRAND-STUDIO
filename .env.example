###############################################
# Environment Configuration Example (Sanitized)
# Copy to .env and fill in real secret values.
###############################################

# Database
DATABASE_URL=postgresql://USER:PASSWORD@HOST/DB_NAME?sslmode=require

# Stack Auth / Project
STACK_SECRET_SERVER_KEY=replace_me
VITE_STACK_PUBLISHABLE_CLIENT_KEY='replace_me'
VITE_STACK_PROJECT_ID=project-uuid-here

# Google Veo (Primary Video Generation)
GOOGLE_API_KEY=replace_me
VEO_GOOGLE_MODEL=veo-001

# Legacy Replicate (Fallback)
REPLICATE_API_TOKEN=replace_me
REPLICATE_USERNAME=your_replicate_username
VEO_MODEL_VERSION=latest

# Anthropic (Maya chat)
ANTHROPIC_API_KEY=replace_me

# AWS S3
AWS_ACCESS_KEY_ID=replace_me
AWS_SECRET_ACCESS_KEY=replace_me
AWS_REGION=us-east-1
AWS_S3_BUCKET=your-bucket-name

# Admin / Impersonation
ADMIN_USER_ID=
SHANNON_USER_ID=

# Email / Notifications
FLODESK_API_KEY=replace_me
RESEND_API_KEY=replace_me

# Meta / Instagram
INSTAGRAM_BUSINESS_ACCOUNT_ID=
META_ACCESS_TOKEN=

# ManyChat
MANYCHAT_API_TOKEN=

# Make.com (automation)
MAKE_API_TOKEN=

# Payments (Stripe)
STRIPE_SECRET_KEY=replace_me
TESTING_VITE_STRIPE_PUBLIC_KEY=replace_me

# Runtime
NODE_ENV=development
PORT=5000
REPLIT_DEV_DOMAIN=localhost

# Optional Feature Flags
VEO_DEBUG=0

###############################################
# Notes:
# - NEVER commit real secrets. Add .env to .gitignore (if not already).
# - Rotate credentials if previously committed.
# - Frontend-exposed vars (Vite) must be prefixed with VITE_.
###############################################