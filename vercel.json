{
  "buildCommand": "npm run build",
  "outputDirectory": "client/dist",
  "redirects": [
    { "source": "/:path*", "destination": "https://sselfie.ai/:path*", "permanent": true }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
