{
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/training-completion-monitor",
      "schedule": "*/2 * * * *"
    },
    {
      "path": "/api/cron/generation-completion-monitor",
      "schedule": "* * * * *"
    }
  ]
}